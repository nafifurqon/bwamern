\nimport DetailsPage from \"pages/DetailsPage\";\nimport Checkout from 'pages/Checkout';\nimport { ToastContainer } from 'react-toastify';\n\nfunction App() {\n  return (\n    <div className=\"App\">\n      <Router>\n          <Route exact path=\"/\" component={LandingPage} />\n          <Route exact path=\"/properties/:id\" component={DetailsPage} />\n          <Route exact path=\"/checkout\" component={Checkout} />\n      </Router>\n\n      <ToastContainer></ToastContainer>\n    </div>\n  );\n}\n\nexport default App;\n","/home/nafifurqon/2019/belajar/bwa/kelas-mern/bwamern/src/pages/LandingPage.js",[],"/home/nafifurqon/2019/belajar/bwa/kelas-mern/bwamern/src/parts/Header.js",[],"/home/nafifurqon/2019/belajar/bwa/kelas-mern/bwamern/src/elements/Button/index.js",["195"],"import React from 'react'\nimport { Link } from 'react-router-dom'\nimport propTypes from 'prop-types'\n\nexport default function Button(props) {\n    const className = [props.className]\n    if(props.isPrimary) className.push(\"btn-primary\");\n    if (props.isLight) className.push(\"btn-light\");\n    if(props.isLarge) className.push(\"btn-lg\");\n    if(props.isSmall) className.push(\"btn-sm\");\n    if(props.isBlock) className.push(\"btn-block\");\n    if(props.hasShadow) className.push(\"btn-shadow\");\n\n    const onClick = () => {\n        if(props.onClick) props.onClick()\n    };\n\n    if(props.isDisabled || props.isLoading){\n        if(props.isDisabled) className.push(\"disabled\");\n        return (\n            <span\n                className={className.join(\" \")} \n                style={props.style}\n            >\n                {props.isLoading ? (\n                    <>\n                        <span className=\"spinner-border spinner-border-sm mx-5\"></span>\n                        <span className=\"sr-only\">Loading...</span>\n                    </>\n                ) : (\n                    props.children\n                )}\n            </span>\n        );\n    }\n    \n\n    if(props.type === \"link\"){\n        if(props.isExternal) {\n            return (\n                <a \n                    href={props.href} \n                    className={className.join(\" \")} \n                    style={props.style} \n                    target={props.target === \"_blank\" ? \"_blank\" : undefined}\n                    rel={props.target === \"_blank\" ? \"noopener noreferer\" : undefined}\n                >\n                    {props.children}\n                </a>\n        ) \n        } else {\n            return (\n                <Link\n                    to={props.href} \n                    className={className.join(\" \")} \n                    style={props.style}\n                    onClick={onClick} \n                >\n                    {props.children}\n                </Link>\n            )\n        }\n    }\n\n    return (\n        <button\n            className={className.join(\" \")} \n            style={props.style}\n            onClick={onClick}\n        >\n            {props.children}\n        </button>\n    )\n}\n\nButton.propTypes = {\n    type: propTypes.oneOf([\"button\", \"link\"]),\n    onClick: propTypes.func,\n    href: propTypes.string,\n    target: propTypes.string,\n    className: propTypes.string,\n    isPrimary: propTypes.bool,\n    isLight: propTypes.bool,\n    isExternal: propTypes.bool,\n    isDisabled: propTypes.bool,\n    isLoading: propTypes.bool,\n    isSmall: propTypes.bool,\n    isLarge: propTypes.bool,\n    isBlock: propTypes.bool,\n    hasShadow: propTypes.bool\n}","/home/nafifurqon/2019/belajar/bwa/kelas-mern/bwamern/src/parts/IconText.js",[],"/home/nafifurqon/2019/belajar/bwa/kelas-mern/bwamern/src/parts/Hero.js",[],"/home/nafifurqon/2019/belajar/bwa/kelas-mern/bwamern/src/utils/formatNumber.js",["196"],"export default number => {\n    const formatNumbering = new Intl.NumberFormat(\"id-ID\");\n    return formatNumbering.format(number);\n}","/home/nafifurqon/2019/belajar/bwa/kelas-mern/bwamern/src/parts/MostPicked.js",[],"/home/nafifurqon/2019/belajar/bwa/kelas-mern/bwamern/src/parts/Categories.js",[],"/home/nafifurqon/2019/belajar/bwa/kelas-mern/bwamern/src/elements/Star/index.js",[],"/home/nafifurqon/2019/belajar/bwa/kelas-mern/bwamern/src/parts/Testimony.js",[],"/home/nafifurqon/2019/belajar/bwa/kelas-mern/bwamern/src/parts/Footer.js",[],"/home/nafifurqon/2019/belajar/bwa/kelas-mern/bwamern/src/elements/Form/InputNumber/index.js",["197"],"import React, { useState } from 'react';\nimport propTypes from \"prop-types\";\nimport './index.scss';\n\nexport default function Number(props) {\n    const { value, placeholder, name, min, max, prefix, suffix, isSuffixPlural } = props;\n    \n    // const [InputValue, setInputValue] = useState(`${prefix}${value}${suffix}`);\n\n    const onChange = (e) => {\n        let value = String(e.target.value);\n        // if (prefix) value = value.replace(prefix);\n        // if (suffix) value = value.replace(suffix);\n\n        // const patternNumeric = new RegExp(\"[0-9]*\");\n        // const isNumeric = patternNumeric.test(value);\n\n        // if (isNumeric && +value <= max && +value >= min ){\n        if (+value <= max && +value >= min ){\n            props.onChange({\n                target: {\n                    name: name,\n                    value: +value\n                }\n            });\n            // setInputValue(`${prefix}${value}${suffix}${isSuffixPlural && value > 1 ? \"s\" : \"\"}`);\n        }\n    };\n\n    const minus = () => {\n        value > min &&\n            onChange({\n                target: {\n                    name: name,\n                    value: +value - 1\n                }\n            });\n    };\n\n    const plus = () => {\n        value < max &&\n            onChange({\n                target: {\n                    name: name,\n                    value: +value + 1\n                }\n            });\n    };\n\n    return (\n        <div className={[\"input-number mb-3\", props.outerClassName].join(\" \")}>\n            <div className=\"input-group\">\n                <div className=\"input-group-prepend\">\n                    <span className=\"input-group-text minus\" onClick={minus}>\n                        -\n                    </span>\n                </div>\n                <input \n                    min={min}\n                    mix={max}\n                    name={name}\n                    readOnly\n                    pattern=\"[0-9]*\"\n                    className=\"form-control\"\n                    placeholder={placeholder ? placeholder : \"0\"}\n                    value={`${prefix}${value}${suffix}${isSuffixPlural && value > 1 ? \"s\" : \"\"}`}\n                    onChange={onChange}\n                />\n                <div className=\"input-group-append\">\n                    <span className=\"input-group-text plus\" onClick={plus}>\n                        +\n                    </span>\n                </div>\n            </div>\n        </div>\n    )\n}\n\nNumber.defaultProps = {\n    min: 1,\n    max: 1,\n    prefix: \"\",\n    suffix: \"\"\n};\n\nNumber.propTypes = {\n    value: propTypes.oneOfType([propTypes.string, propTypes.number]),\n    onChange: propTypes.func,\n    placeholder: propTypes.string,\n    outerClassName: propTypes.string,\n    isSuffixPlural: propTypes.bool\n};","/home/nafifurqon/2019/belajar/bwa/kelas-mern/bwamern/src/pages/DetailsPage.js",[],"/home/nafifurqon/2019/belajar/bwa/kelas-mern/bwamern/src/parts/PageDetailTitle.js",[],"/home/nafifurqon/2019/belajar/bwa/kelas-mern/bwamern/src/elements/Breadcrumb/index.js",[],"/home/nafifurqon/2019/belajar/bwa/kelas-mern/bwamern/src/parts/FeaturedImage.js",[],"/home/nafifurqon/2019/belajar/bwa/kelas-mern/bwamern/src/parts/PageDetailDescription.js",[],"/home/nafifurqon/2019/belajar/bwa/kelas-mern/bwamern/src/parts/BookingForm.js",["198"],"import React, { Component } from 'react'\nimport { withRouter } from 'react-router-dom'\nimport propTypes from 'prop-types'\nimport Button from 'elements/Button'\nimport { InputNumber, InputDate } from 'elements/Form'\n\nclass BookingForm extends Component {\n    constructor(props) {\n        super(props);\n        this.state = {\n            data: {\n                duration: 1,\n                date: {\n                    startDate: new Date(),\n                    endDate: new Date(),\n                    key: \"selection\"\n                }\n            }\n        }\n    }\n\n    updateData = (e) => {\n        this.setState({\n            ...this.state,\n            data: {\n                ...this.state.data,\n                [e.target.name]: e.target.value\n            }\n        })\n    }\n\n    componentDidUpdate(prevProps, prevState) {\n        const { data } = this.state\n\n        if(prevState.data.date !== data.date) {\n            const startDate = new Date(data.date.startDate);\n            const endDate = new Date(data.date.endDate);\n            const countDuration = new Date(endDate - startDate).getDate();\n\n            this.setState({\n                data: {\n                    ...this.state.data,\n                    duration: countDuration\n                }\n            })\n        }\n\n        if(prevState.data.duration !== data.duration) {\n            const startDate = new Date(data.date.startDate);\n            const endDate = new Date(\n                startDate.setDate(startDate.getDate() + +data.duration - 1)\n            );\n            this.setState({\n                ...this.state,\n                data: {\n                    ...this.state.data,\n                    date: {\n                        ...this.state.data.date,\n                        endDate: endDate\n                    }\n                }\n            })\n        }\n    }\n    \n    startBooking = () => {\n        const { data } = this.state;\n        this.props.startBooking({\n          _id: this.props.itemDetails._id,\n          duration: data.duration,\n          date: {\n            startDate: data.date.startDate,\n            endDate: data.date.endDate,\n          },\n        });\n        this.props.history.push('/checkout');\n      }\n\n    render() {\n        const { data } = this.state;\n        const { itemDetails, startBooking } = this.props\n        \n        return (\n            <div className=\"card bordered\" style={{ padding: '60px 80px' }}>\n                <h4 className=\"mb-3\">Start Booking</h4>\n                <h5 className=\"h2 text-teal mb-4\">\n                    ${itemDetails.price}{\" \"}\n                    <span className=\"text-gray-500 font-weight-light\">\n                        per {itemDetails.unit}\n                    </span>\n                </h5>\n\n                <label htmlFor=\"duration\">How long you will stay?</label>\n                <InputNumber \n                    max={30}\n                    suffix={\" night\"}\n                    isSuffixPlural\n                    onChange={this.updateData}\n                    name=\"duration\"\n                    value={data.duration}\n                />\n\n                <label htmlFor=\"date\">Pick a date   </label>\n                <InputDate\n                    onChange={this.updateData}\n                    name=\"date\"\n                    value={data.date}\n                />\n\n                <h6 className=\"text-gray-500 font-weight-light\" style={{ marginBottom: 40 }}>\n                    You will pay{\" \"}\n                    <span className=\"text-gray-900\">\n                        ${itemDetails.price * data.duration} USD\n                    </span>{\" \"}\n                    per{\" \"}\n                    <span className=\"text-gray-900\">\n                        {data.duration} {itemDetails.unit}\n                    </span>\n                </h6>\n\n                <Button\n                    className=\"btn\"\n                    hasShadow\n                    isPrimary\n                    isBlock\n                    onClick={this.startBooking}\n                >\n                    Continue to Book\n                </Button>\n            </div>\n        )\n    }\n}\n\nBookingForm.propTypes = {\n    itemDetails: propTypes.object,\n    startBooking: propTypes.func\n}\n\nexport default withRouter(BookingForm);","/home/nafifurqon/2019/belajar/bwa/kelas-mern/bwamern/src/elements/Form/index.js",[],"/home/nafifurqon/2019/belajar/bwa/kelas-mern/bwamern/src/elements/Form/InputDate/index.js",[],"/home/nafifurqon/2019/belajar/bwa/kelas-mern/bwamern/src/utils/formatDate.js",["199"],"export default date => {\n    const d = new Date(date);\n    const dtf = new Intl.DateTimeFormat(\"en\", {\n        year: \"numeric\",\n        month: \"short\",\n        day: \"2-digit\"\n    });\n    const [{value: mo}, , {value: da}] = dtf.formatToParts(d);\n\n    return `${da} ${mo}`;\n};\n\n",["200","201"],"/home/nafifurqon/2019/belajar/bwa/kelas-mern/bwamern/src/pages/Checkout.js",[],"/home/nafifurqon/2019/belajar/bwa/kelas-mern/bwamern/src/parts/Checkout/BookingInformation.js",[],"/home/nafifurqon/2019/belajar/bwa/kelas-mern/bwamern/src/parts/Checkout/Payment.js",[],"/home/nafifurqon/2019/belajar/bwa/kelas-mern/bwamern/src/parts/Checkout/Completed.js",[],"/home/nafifurqon/2019/belajar/bwa/kelas-mern/bwamern/src/elements/Stepper/index.js",[],"/home/nafifurqon/2019/belajar/bwa/kelas-mern/bwamern/src/elements/Form/InputText/index.js",[],"/home/nafifurqon/2019/belajar/bwa/kelas-mern/bwamern/src/elements/Form/InputFile/index.js",[],"/home/nafifurqon/2019/belajar/bwa/kelas-mern/bwamern/src/elements/Stepper/Controller/index.js",[],"/home/nafifurqon/2019/belajar/bwa/kelas-mern/bwamern/src/elements/Stepper/Numbering/index.js",[],"/home/nafifurqon/2019/belajar/bwa/kelas-mern/bwamern/src/elements/Stepper/MainContent/index.js",[],"/home/nafifurqon/2019/belajar/bwa/kelas-mern/bwamern/src/elements/Stepper/Meta/index.js",[],"/home/nafifurqon/2019/belajar/bwa/kelas-mern/bwamern/src/store/index.js",[],"/h